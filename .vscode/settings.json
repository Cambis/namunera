{
  "deno.path": "/usr/local/bin/deno",
  "deno.enablePaths": [
    "./packages/frontend"
  ],
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "deno.unstable": true,
  "deno.cache": "./packages/frontend/.deno",
  "deno.importMap": "./packages/frontend/import_map.json",
  "deno.config": "./packages/frontend/deno.json",
  "tailwindCSS.includeLanguages": {
    "typescriptreact": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    ["cn\\(([^)]*)\\)", "'([^']*)'"],
    ["classNames\\(([^)]*)\\)", "'([^']*)'"],
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "classNames"
  ],
  "tailwindCSS.experimental.configFile": null
}
